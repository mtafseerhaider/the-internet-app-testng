<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="LoginTestsPositive" verbose="1" parallel="tests" thread-count="2">

	<test name="Valid Username and Password - Chrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="LoginTestsPositive">
			</class>
		</classes>
	</test>

	<test name="Valid Username and Password- Firefox">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="LoginTestsPositive">
			</class>
		</classes>
	</test>
	
</suite>
